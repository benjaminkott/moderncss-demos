{
    "name": "bk2k/modern-css-demos",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Benjamin Kott",
            "email": "benjamin.kott@outlook.com"
        }
    ],
    "require": {
        "doctrine/collections": "^2.1",
        "doctrine/common": "^3.4",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/asset": "^6.3",
        "symfony/cache": "^6.3",
        "symfony/finder": "^6.3",
        "symfony/flex": "^2.3",
        "symfony/form": "^6.3",
        "symfony/http-kernel": "^6.3",
        "symfony/mime": "^6.3",
        "symfony/runtime": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^6.3",
        "twig/extra-bundle": "^3.6",
        "twig/twig": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cgl:fix": [
            "php-cs-fixer fix"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21",
        "symfony/debug-bundle": "^6.3",
        "symfony/maker-bundle": "^1.49"
    }
}
